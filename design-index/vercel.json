{
  "builds": [
    { "src": "api/*.go", "use": "@vercel/go" },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/feature_tool.go" },
    { "src": "/(.*)", "dest": "/build/$1" }
  ]
}
